{
  "manifest_version": 2,

  "name": "GTmetrix Analyzer Plugin",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [{
    "js": ["./dist/bundle.js"],
    "matches": ["https://app.slack.com/*"]
  }]
}
