{
  "manifest_version": 2,

  "name": "Slack Saver",
  "description": "Slack Saver",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "web_accessible_resources": ["script.js"],

  "permissions": ["activeTab"],

  "content_scripts": [{
    "js": ["index.js"],
    "matches": ["https://app.slack.com/*"]
  }]
}
